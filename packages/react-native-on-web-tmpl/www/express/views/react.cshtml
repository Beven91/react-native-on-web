@html.raw(model.fontface)
@html.raw(model.stylesheet)
<div id="app">@html.raw(model.initialHTML)</div>
@{
  var name='\'@@__reactAppContext__@@\'';
}
<script type="text/javascript">
  window[@html.raw(name)]=@html.raw(model.reactAppContext)
</script>
@{
  var jsAssets = (model.Webpack || {}).jsAssets || [];
  jsAssets.forEach(function(js){
    <script type="text/javascript" src="@js"></script>
  })
}
@{
  if(model.splitRouteJs){
    <script type="text/javascript" src="@model.splitRouteJs"></script>
  }
}